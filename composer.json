{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "cweagans/composer-patches": "^1.7",
        "darkaonline/l5-swagger": "^8.5",
        "google/cloud-logging": "^1.29",
        "google/cloud-storage": "^1.36",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^0.6.3",
        "laravel/framework": "^10.10",
        "laravel/passport": "^12.2",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.12",
        "laravel/tinker": "^2.8",
        "mongodb/laravel-mongodb": "^4.0",
        "pinkary-project/type-guard": "^0.1.0",
        "spatie/laravel-analytics": "^5.3",
        "spatie/laravel-data": "^4.1",
        "stackkit/laravel-google-cloud-tasks-queue": "^3.7",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "larastan/larastan": "^2.0",
        "laravel/breeze": "^1.28",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "suggest": {
        "laravel/pail": "For better local debugging"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": ["app/DTO/Telegram/Objects.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ]
    },
    "extra": {
        "patches": {
            "spatie/laravel-analytics": {
                "Optional credentials file": "patches/laravel-analytics/optional-credentials-file.patch"
            }
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "cweagans/composer-patches": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "provide" : {
        "ext-mongodb": "*"
    }
}
